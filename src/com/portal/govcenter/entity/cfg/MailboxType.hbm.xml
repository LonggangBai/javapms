<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.portal.govcenter.entity">
	<class name="MailboxType" table="tq_mailbox_type">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="integer" column="type_id"><generator class="identity"/></id>
		<property name="name" column="name" type="string" not-null="true" length="20"/>
		<property name="priority" column="priority" type="integer" not-null="true" length="10"/>
		<many-to-one name="site" column="site_id" class="com.portal.sysmgr.entity.Site" not-null="true"></many-to-one>
	</class>
</hibernate-mapping>